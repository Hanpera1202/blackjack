<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.bj"
    android:versionCode="1"
    android:versionName="1.0">

    <!-- Include required permissions for Google Mobile Ads to run-->
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.VIBRATE" android:required="false"></uses-permission>

    <!-- For Growth Push -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <permission android:name="com.ivoid.bj.permission.C2D_MESSAGE" android:protectionLevel="signature" />
    <uses-permission android:name="com.ivoid.bj.permission.C2D_MESSAGE" />


    <application android:name="com.ivoid.bj.Game"
    			  android:icon="@drawable/icon"
    			  android:label="@string/app_name">
        <!--This meta-data tag is required to use Google Play Services.-->
        <meta-data android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <activity
            android:name="com.ivoid.bj.Dealer"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ivoid.bj.Title"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>
        <activity
            android:name="com.ivoid.bj.Bonus"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>
        <activity android:name="com.ivoid.bj.Competition"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>
        <activity android:name="com.ivoid.bj.Result"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>
        <activity android:name="com.ivoid.bj.ResultDialog"
            android:theme="@android:style/Theme.Dialog"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>
        <activity android:name="com.ivoid.bj.MyData"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>
        <activity android:name="com.ivoid.bj.Setting"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
            android:screenOrientation="portrait"
            android:configChanges="orientation|keyboardHidden"
            android:launchMode="singleTask">
        </activity>

        <!--Include the AdActivity configChanges and theme. -->
        <activity android:name="com.google.android.gms.ads.AdActivity"
            android:configChanges="keyboard|keyboardHidden|orientation|screenLayout|uiMode|screenSize|smallestScreenSize"
            android:theme="@android:style/Theme.Translucent" />

        <!-- For Growth Push -->
        <activity android:name="com.growthpush.view.AlertActivity" android:configChanges="orientation|keyboardHidden" android:launchMode="singleInstance" android:theme="@android:style/Theme.Translucent" />
        <receiver android:name="com.growthpush.BroadcastReceiver" android:permission="com.google.android.c2dm.permission.SEND">
            <intent-filter>
            <action android:name="com.google.android.c2dm.intent.RECEIVE" />
            <action android:name="com.google.android.c2dm.intent.REGISTRATION" />
            <category android:name="com.ivoid.bj" />
        </intent-filter>
        </receiver>

     </application>
</manifest>